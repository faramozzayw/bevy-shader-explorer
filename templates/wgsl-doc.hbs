<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{{filename}}</title>
    <script src="https://cdn.jsdelivr.net/npm/fuse.js@6.6.2"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/handlebars@latest/dist/handlebars.min.js"
    ></script>
    <script src="/public/search.js" type="text/javascript"></script>

    <link rel="stylesheet" href="/public/styles.css" />
    <link rel="icon" href="/public/favicon.ico" type="image/svg+xml" />
  </head>
  <body>
    <header>
      <div>
        <a href="/" class="header-link">Back home üè°</a>
        <a
          href="https://www.w3.org/TR/WGSL"
          target="_blank"
          rel="noopener noreferrer"
          class="header-link"
        >WGSL Spec <img src="/public/wgsl.png" width="20" height="20" /></a>
        <a
          href="https://github.com/bevyengine/bevy"
          target="_blank"
          rel="noopener noreferrer"
          class="header-link"
        >Bevy <img src="/public/github.png" width="20" height="20" /></a>
        <a
          href="https://webgpufundamentals.org/webgpu/lessons/webgpu-wgsl-function-reference.html"
          target="_blank"
          rel="noopener noreferrer"
          class="header-link"
        >WGSL Functions</a>
        <input
          type="text"
          id="search-input"
          class="search-input"
          placeholder="Search... üîç"
        />
      </div>
      <div id="results"></div>
    </header>

    <h1>
      <code>{{filename}}</code>
      <a href="{{githubLink}}" target="_blank" rel="noopener noreferrer">
        <img src="/public/github.png" width="20" height="20" />
      </a>
    </h1>

    {{#if importPath.length}}
      <div class="importPath">
        <h3>Import path</h3>
        <button onclick="navigator.clipboard.writeText('{{importPath}}')">
          {{importPath}}
          <small>(click to copy)</small>
        </button>
      </div>
    {{/if}}

    {{#if consts.length}}
      <h3>Constants</h3>
    {{/if}}

    {{#if consts.length}}
      <div class="function">
        <table class="params-table">
          <thead>
            <tr>
              <th>Name</th>
              <th>Type</th>
              <th>Value</th>
            </tr>
          </thead>

          {{#each consts}}
            <tbody>
              <tr>
                <td class="flex-align-center">{{name}}
                  <a href="#{{name}}">#</a>
                  <a
                    href="{{@root.githubLink}}#L{{lineNumber}}"
                    target="_blank"
                    rel="noopener noreferrer"
                  >
                    <img src="/public/github.png" width="20" height="20" />
                  </a>
                </td>
                <td class="param-type param-type-table">
                  {{#if typeLink}}
                    <a
                      href="{{typeLink}}"
                      target="_blank"
                      rel="noopener noreferrer"
                      class="param-type img-link"
                    >
                      {{type}}
                      {{#contains "w3.org" typeLink}}
                        <img src="/public/wgsl.png" width="20" height="20" />
                      {{/contains}}
                    </a>
                  {{else}}
                    {{type}}
                  {{/if}}
                </td>
                <td>{{value}}</td>
              </tr>
            </tbody>
          {{/each}}
        </table>
      </div>
    {{/if}}

    {{#if bindings.length}}
      <h3>Bindings</h3>
    {{/if}}

    {{#if bindings.length}}
      <div class="function">
        <table class="params-table">
          <thead>
            <tr>
              <th>Name</th>
              <th>Group</th>
              <th>Binding index</th>
              <th>Binding type</th>
              <th>Type</th>
            </tr>
          </thead>

          {{#each bindings}}
            <tbody>
              <tr>
                <td class="flex-align-center">{{name}}
                  <a href="#{{name}}">#</a>
                  <a
                    href="{{@root.githubLink}}#L{{lineNumber}}"
                    target="_blank"
                    rel="noopener noreferrer"
                  >
                    <img src="/public/github.png" width="20" height="20" />
                  </a>
                </td>
                <td>{{groupIndex}}</td>
                <td>{{bindingIndex}}</td>
                <td>{{bindingType}}</td>
                <td class="param-type param-type-table">
                  {{#if typeLink}}
                    <a
                      href="{{typeLink}}"
                      target="_blank"
                      rel="noopener noreferrer"
                      class="param-type img-link"
                    >
                      {{type}}
                      {{#contains "w3.org" typeLink}}
                        <img src="/public/wgsl.png" width="20" height="20" />
                      {{/contains}}
                    </a>
                  {{else}}
                    {{type}}
                  {{/if}}
                </td>
 
              </tr>
            </tbody>
          {{/each}}
        </table>
      </div>
    {{/if}}

    {{#if structures.length}}
      <h3>Structures</h3>
    {{/if}}

    {{#each structures}}
      <div class="function">
        <div class="function-header">
          <div>
            <h3 class="function-name" id="{{name}}">
              {{name}}
            </h3>
            <a href="#{{name}}">#</a>
            <a
              href="{{@root.githubLink}}#L{{lineNumber}}"
              target="_blank"
              rel="noopener noreferrer"
            >
              <img src="/public/github.png" width="20" height="20" />
            </a>
          </div>
        </div>

        {{#if comment}}
          <div class="function-comment">{{{linkify
              (code-highlight comment)
            }}}</div>
        {{/if}}

        {{#if fields.length}}
          <table class="params-table">
            <thead>
              <tr>
                {{#if hasAnnotations}}<th>Annotation</th>{{/if}}
                <th>Name</th>
                <th>Type</th>
              </tr>
            </thead>
            <tbody>
              {{#each fields}}
                <tr>
                  {{#if annotation.length}}
                    <td class="annotation">
                      {{annotation}}
                    </td>
                  {{/if}}
                  <td>{{name}}</td>
                  <td class="param-type param-type-table">
                    {{#if typeLink}}
                      <a
                        href="{{typeLink}}"
                        target="_blank"
                        rel="noopener noreferrer"
                        class="param-type img-link"
                      >
                        {{type}}
                        {{#contains "w3.org" typeLink}}
                          <img src="/public/wgsl.png" width="20" height="20" />
                        {{/contains}}
                      </a>
                    {{else}}
                      {{type}}
                    {{/if}}
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        {{/if}}

      </div>
    {{/each}}

    {{#if functions.length}}
      <h3>Functions</h3>
    {{/if}}

    {{#each functions}}
      <div class="function">
        <div class="function-header">
          <div>
            <h3 class="function-name" id="{{name}}">{{name}}</h3>
            <a href="#{{name}}">#</a>
            <a
              href="{{@root.githubLink}}#L{{lineNumber}}"
              target="_blank"
              rel="noopener noreferrer"
            >
              <img src="/public/github.png" width="20" height="20" />
            </a>
          </div>

          {{#if stageAttribute}}
            <div>
              <span
                class="attribute-badge {{stageAttribute}}-badge"
              >@{{stageAttribute}}</span>
            </div>
          {{/if}}
        </div>

        {{#if comment}}
          <div class="function-comment">{{{linkify
              (code-highlight comment)
            }}}</div>
        {{/if}}

        <div class="signature">
          <span class="kw">{{#if visibility}}{{visibility}} {{/if}}fn</span>
          <span class="fn-name">{{name}}</span>(
          <span>
            {{#each params}}
              <span class="param-name">{{name}}</span>{{#if type}}:
                <span class="param-type">{{type}}</span>{{/if}}{{#unless
                @last
              }}, {{/unless}}
            {{/each}}
          </span>
          )
          {{#if (neq returnType "void")}}
            ->
            <span class="return-type-val">{{returnType}}</span>{{/if}}
        </div>

        {{#if params.length}}
          <table class="params-table">
            <thead>
              <tr>
                <th>Parameter</th>
                <th>Type</th>
              </tr>
            </thead>
            <tbody>
              {{#each params}}
                <tr>
                  <td>{{name}}</td>
                  <td class="param-type">
                    {{#if typeLink}}
                      <a
                        href="{{typeLink}}"
                        target="_blank"
                        rel="noopener noreferrer"
                        class="param-type img-link"
                      >
                        {{type}}
                        {{#contains "w3.org" typeLink}}
                          <img src="/public/wgsl.png" width="20" height="20" />
                        {{/contains}}
                      </a>
                    {{else}}
                      {{type}}
                    {{/if}}
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        {{/if}}

        {{#if (neq returnType "void")}}
          <div class="return-type">Returns:
            {{#if returnTypeLink}}
              <a
                href="{{returnTypeLink}}"
                target="_blank"
                rel="noopener noreferrer"
                class="return-type-val img-link"
              >
                {{returnType}}
                {{#contains "w3.org" returnTypeLink}}
                  <img src="/public/wgsl.png" width="20" height="20" />
                {{/contains}}
              </a>
            {{else}}
              <span class="return-type-val">{{returnType}}</span>
            {{/if}}</div>

        {{/if}}
      </div>
    {{/each}}
  </body>
</html>
